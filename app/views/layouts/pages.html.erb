<!DOCTYPE html>
<html>
<head>
  <title><%=@title%></title>
  <!--%= stylesheet_link_tag :all %-->
  <%= stylesheet_link_tag 'flexible-grids.css', :media => 'screen' %>
  <%= stylesheet_link_tag 'display', :media => 'screen' %>
  <%= stylesheet_link_tag 'colorbox', :media => 'screen' %>
  <%= javascript_include_tag :defaults %>

  <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script> 

  <%= javascript_include_tag '/javascripts/jquery.colorbox-min.js' %>
  <%= javascript_include_tag 'display.js' %>

 
  <%= csrf_meta_tag %>



</head>
<body>


<header>
	<div class="ym-wrapper">
		<div class="ym-wbox">
			<span class="logo">A Food Map</span>
		</div>
	</div>
</header>
<nav id="nav">
	<div class="ym-wrapper">
		<div class="ym-hlist">
	
			
		</div>
	</div>
</nav>
<div id="main">
 <div class="ym-wrapper">
    <%= yield %>
 </div>
</div>
<footer>
	<div class="ym-wrapper">
		<div class="ym-wbox">
		</div>
	</div>
</footer>


</body>
</html>  
