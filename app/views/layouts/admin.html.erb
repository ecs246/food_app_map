<!DOCTYPE html>
<html>
<head>
  <title>FoodMapApp</title>
  <!--%= stylesheet_link_tag :all %-->
<%= stylesheet_link_tag '/jquery-ui-1.8.16.custom/css/ui-lightness/jquery-ui-1.8.20.custom.css', :media => 'screen' %>


  <%= stylesheet_link_tag 'flexible-grids.css', :media => 'screen' %>
  <%= stylesheet_link_tag 'custom', :media => 'screen' %>
  <%= stylesheet_link_tag 'colorbox', :media => 'screen' %>
 
  
  <%= javascript_include_tag :defaults %>

  <!--script src="/javascripts/jquery.colorbox-min.js" type="text/javascript"></script-->
  <%= javascript_include_tag '/javascripts/jquery.colorbox-min.js' %>

  <%= javascript_include_tag 'admin' %>
  <%= javascript_include_tag 'category' %>
  <%= javascript_include_tag 'gallery' %>
  <%= javascript_include_tag 'image_utility' %>
 
  
  <%= csrf_meta_tag %>



</head>
<body>


<header>
	<div class="ym-wrapper">
		<div class="ym-wbox">
			<span class="logo">A Food Map CMA</span>
		</div>
	</div>
</header>
<nav id="nav">
	<div class="ym-wrapper">
		<div class="ym-hlist">
			<ul>
				<li><a href="/admin/categories">Categories</a></li>
				<li><a href="/admin/images">Images</a></li>
				<li><a href="/admin/image_utility">Image Utility</a></li>
				<li><a href="/admin/vendors">Vendors</a></li>
			</ul>
			
		</div>
	</div>
</nav>
<div id="main">
 <div class="ym-wrapper">
    <%= yield %>
 </div>
</div>
<footer>
	<div class="ym-wrapper">
		<div class="ym-wbox">
                  
		</div>
	</div>
</footer>

</body>
</html>
